install.packages("blogdown") # install the blogdown package
library(blogdown) # load blogdown
setwd('/Users/walentk/Documents/WWW-Resume_page/')
new_site(theme = "wowchemy/starter-academic") # create your website!
file.edit("gitignore")
blogdown::check_site()


blogdown::serve_site()



rstudioapi::navigateToFile("content/authors/admin/_index.md")

git init
git remote add origin https://github.com/walentk/PersonalWebsite.git
git add .
git commit -m "Initial commit of my Blogdown site"
git push -u origin main
